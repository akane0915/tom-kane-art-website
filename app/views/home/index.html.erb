<div class="jumbotron">
  <div class="masthead-text-container">
    <div class="masthead-text">
      <h1>Tom Kane</h1>
    </div>
  </div>
</div>

<p id="notice"><%= notice %></p>

<ul class="gallery grid effect-2" id="grid">

  <% @paintings.each do |painting| %>
    <%= link_to( image_tag("#{painting.image}", alt: "#{painting.title}", :class=>"#{painting.style}"), painting) %>
    <br><br><br><br>
  <% end %>

</ul>
